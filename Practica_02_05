	<script type="text/javascript">
		function ocultarModal(){
			$('.modal').fadeOut();

		}
		function mostrarModal(){
			$('.modal').fadeIn();
		}
		function iniciar(){
			$('.link-up').click(mostrarModal);
			$('.cerrar').click(ocultarModal); 
		}
		$(document).ready(iniciar);
	</script>
</head>
